{
  "name": "r6-customs",
  "description": "a simple discord bot for r6 customs matchmaking",
  "author": "saudalnasser",
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "bot",
    "r6",
    "customs",
    "matchmaking"
  ],
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "start:dev": "ts-node-dev --inspect=9229 --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy:guild": "tsc && node dist/scripts/deploy-commands-guild.js",
    "deploy:global": "tsc && node dist/scripts/deploy-commands-global.js"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "dependencies": {
    "@discordjs/rest": "^0.5.0",
    "@r6stats/node": "^4.0.0-alpha3",
    "axios": "^0.27.2",
    "colorette": "^2.0.19",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "mongoose": "^6.4.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
